//Lê um número e escreve seu sucessor e seu antecessor na tela

//Inclusao de bibliotecas
#include <stdio.h>

//Função principal
int main()
{
	//Declaraçao de variaveis
	int n1;
	
	//Entrada de dados
    printf("Digite o valor\n");
    scanf("%d", &n1);
    
	//Saída de dados
	printf("Numero digitado: %d\nSucessor: %d\nAntecessor: %d\n", n1, n1 + 1, n1 - 1);	
	
	//Fim da função principal
	return 0;
}
