//Lê um número e mostra a terça parte desse número

//Inclusao de bibliotecas
#include <stdio.h>

//Função principal
int main()
{
	//Declaraçao de variaveis
	float n1, result;
	
	//Entrada de dados
    printf("Digite o valor\n");
    scanf("%f", &n1);
    
    //Cálculo
    result = n1 / 3;
    
	//Saída de dados
	printf("%.2f / 3 = %.2f\n", n1, result);	
	
	//Fim da função principal
	return 0;
}
